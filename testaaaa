$(function() {var videohtml5 = $('.videoGallery .videohtml5');
var liHeight = $('.videoGallery li').height();

// BLOGGER
videohtml5.click(function(){
var videoID = $(this).attr('data-videoID');
var videos = $('<div class="meuVideo"><center> <object class="BLOG_video_class" contentid="5cd2555e6f3a6557" height="450" id="BLOG_video-5cd2555e6f3a6557" width="100%"></object></center> </div>');

$('.meuVideo, .nowPlaying').remove();
$(this).parents().eq(2).append(videos);

});


 

})
